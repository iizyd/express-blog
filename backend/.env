# 项目配置
PORT=9000
DEFAULT_PAGE_SIZE=10
MAX_PAGE_SIZE=100
LOG_LEVEL='error' # error < warn < info < verbose < debug < silly
ENABLE_CONSOLE_LOG=0 # 0 or 1

